QFrame{
	border-radius:5px;
	background:rgb(220,220,220);
}
QToolButton{
	border:none;
}
QToolButton:hover{
	border:none;
	border-radius:12px;
	background-color:rgb(250,250,250);
}
QLabel{
	border:none;
	color:rgb(150,150,150);
}
QLabel:hover{
	border:none;
	color:rgb(90,90,90);
}
QLabel:disabled{
	border:none;
	color:rgb(0,0,0);
}